{
  "name": "simple webapp",
  "version": "0.1.0",
  "description": "a simple webapp that uses bower-installer and creates karma, grunt and protrator configs",
  "dependencies": {
    "jquery": "latest",
    "underscore": "latest",
    "backbone": "~1.1.2",
    "backbone.babysitter": "latest",
    "backbone.wreqr": "latest",
    "marionette": "~1.8.7",
    "underscore-contrib": "latest",
    "requirejs": "latest",
    "requirejs-text": "latest",
    "requirejs-hbs": "latest",
    "handlebars": "latest",
    "jasmine-jquery": "1.5.93",
    "bootstrap-sass-official": "latest",
    "breakpoint-sass": "latest",
    "chosen": "https://github.com/harvesthq/chosen/releases/download/v1.1.0/chosen_v1.1.0.zip",
    "bootstrap-chosen": "https://github.com/alxlit/bootstrap-chosen.git",
    "json2": "latest"
  },
  "install": {
    "path": "app/public/bower",
    "sources": {
      "require-handlebars-plugin": "bower_components/require-handlebars-plugin/hbs.js",
      "bootstrap-chosen": [
          "bower_components/bootstrap-chosen/bootstrap-chosen.scss",
          "bower_components/bootstrap-chosen/bootstrap-chosen-variables.scss"
      ],
      "bootstrap-sass-official": "bower_components/bootstrap-sass-official/vendor/assets/**",
      "breakpoint-sass": "bower_components/breakpoint-sass/stylesheets/**",
      "chosen": [
        "bower_components/chosen/chosen.css",
        "bower_components/chosen/chosen-sprite.png",
        "bower_components/chosen/chosen-sprite@2x.png",
        "bower_components/chosen/chosen.jquery.js"
      ],
      "json2": "bower_components/json2/json2.js"
    }
  },
  "authors": [
    "Adam Spence"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    ".*",
    "test/*",
    "tests/*",
    "bower.json",
    "grunt.js",
    "LICENCE",
    "README.md",
    "package.json",
    "build/*",
    "docs/*"
  ]
}
